{
  "name": "buildo-yelp-training",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "avenger": "^7.0.0",
    "fp-ts": "^2.7.1",
    "io-ts": "^2.2.9",
    "io-ts-types": "^0.5.8",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "treat": "^1.5.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^14.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^2.0.2",
    "react-app-rewired": "^2.1.5",
    "react-scripts": "3.4.1",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "start": "yarn stop-cors-proxy && yarn start-cors-proxy && react-app-rewired start",
    "build": "react-app-rewired build",
    "prettier-write": "prettier --parser=typescript \"{**/*,*}.{js,jsx,ts,tsx}\" --write && prettier --parser=css \"{**/*,*}.css\" --write",
    "start-cors-proxy": "docker run -p 3001:8080 --name cors-anywhere -d redocly/cors-anywhere",
    "stop-cors-proxy": "docker stop cors-anywhere || true && docker rm cors-anywhere || true"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
